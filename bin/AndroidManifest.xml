<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.activitydatatransfer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.activitydatatransfer.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.activitydatatransfer.ProcessData"
            android:label="@string/title_activity_process_data"
            android:parentActivityName="com.example.activitydatatransfer.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.activitydatatransfer.MainActivity" />
        </activity>
        <activity
            android:name="com.example.activitydatatransfer.SaveData"
            android:label="@string/title_activity_save_data"
            android:parentActivityName="com.example.activitydatatansfer.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.activitydatatansfer.MainActivity" />
        </activity>
        <activity
            android:name="com.example.activitydatatransfer.ReportActivity"
            android:label="@string/title_activity_report"
            android:parentActivityName="com.example.activitydatatransfer.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.activitydatatransfer.MainActivity" />
        </activity>
    </application>

</manifest>
